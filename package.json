{
  "name": "tb-koa2-server",
  "version": "1.0.0",
  "description": "TB Koa2 Server",
  "scripts": {
    "clean": "rimraf dist",
    "build": "babel src --out-dir dist",
    "serve": "node dist/main",
    "start:babel": "babel-node src/main",
    "start": "cross-env NODE_ENV=develop nodemon --watch src --exec npm run start:babel -L",
    "test": "cross-env NODE_ENV=test ./node_modules/mocha/bin/mocha --compilers js:babel-core/register test/**/*.js --bail -t 2000000",
    "deploy:dev": "pm2 start pm2.config.json --env develop",
    "deploy:prod": "pm2 start pm2.config.json --env production"
  },
  "author": "cody",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.5.2",
    "babel-eslint": "^6.0.4",
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.2.2",
    "cross-env": "^3.2.4",
    "eslint": "^2.4.0",
    "eslint-config-airbnb": "^6.1.0",
    "eslint-plugin-import": "^1.3.0",
    "install": "^0.8.1",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.2"
  },
  "dependencies": {
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-cors": "^0.0.16",
    "koa-multer": "^1.0.2",
    "koa-router": "^7.2.1",
    "koa-swagger-decorator": "^1.4.0",
    "lodash": "^4.17.4",
    "loglevel": "^1.4.0",
    "moment": "^2.18.1",
    "mongoose": "^4.12.3",
    "node-schedule": "^1.3.0",
    "nodemon": "^1.11.0",
    "randomstring": "^1.1.5",
    "request": "^2.81.0",
    "request-promise": "^4.2.0",
    "sha1": "^1.1.1",
    "supertest": "^2.0.1"
  },
  "license": "MIT"
}
